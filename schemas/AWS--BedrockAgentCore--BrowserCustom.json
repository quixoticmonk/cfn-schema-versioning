      "patternProperties": {
        "^[a-zA-Z0-9\\s._:/=+@-]*$": {
          "description": "Value of a tag",
          "maxLength": 256,
          "minLength": 0,
          "pattern": "^[a-zA-Z0-9\\s._:/=+@-]*$",
          "type": "string"
